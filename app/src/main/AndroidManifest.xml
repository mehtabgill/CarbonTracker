    package="ca.cmpt276.carbontracker.UI">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainMenuActivity"></activity>
        <activity android:name=".SelectTransportationModeActivity" />
        <activity android:name=".ViewCarbonFootprintActivity" />
        <activity android:name=".SelectRouteActivity" />
        <activity android:name=".AddRouteActivity" />
        <activity android:name=".EditRouteActivity" />
        <activity android:name=".DeleteRouteActivity" />
        <activity android:name=".AddCarActivity" />
        <activity android:name=".EditDeleteCarActivity" />
